{
  "name": "laravolt/camunda",
  "description": "This package is used to connect Laravolt with Camunda BPMN to make it easier.",
  "keywords": [
    "laravel",
    "laravolt",
    "camunda",
    "bpmn"
  ],
  "homepage": "https://github.com/laravolt/camunda",
  "license": "MIT",
  "authors": [
    {
      "name": "Bayu Hendra Winata",
      "email": "uyab.exe@gmail.com",
      "homepage": "http://id-laravel.com",
      "role": "Developer"
    },
    {
      "name": "Anandia Muhammad Yudhistira",
      "email": "anandiamy@gmail.com",
      "homepage": "http://id-laravel.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": "~5.6 | 6.*",
    "illuminate/database": "^6.4",
    "laravolt/suitable": "^3.6",
    "bensampo/laravel-enum": "^1.26",
    "rkorebrits/htmltoopenxml": "^0.1.9",
    "phpoffice/phpword": "^0.17.0",
    "spatie/data-transfer-object": "^1.10",
    "watson/rememberable": "^3.2",
    "laravolt/semantic-form": "dev-master as 2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Laravolt\\Camunda\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    },
    "laravel": {
      "providers": [
        "Laravolt\\Camunda\\ServiceProvider"
      ],
      "aliases": {
        "Camunda": "Laravolt\\Camunda\\Facade"
      }
    }
  }
}
